{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "[%(asctime)s.%(msecs)03d] %(levelname)-5s (%(filename)s:%(lineno)s) %(message)s",
      "datefmt": "%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "stream": "ext://sys.stdout"
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": [
      "console"
    ]
  }
}
